{
  "name": "tmfe",
  "version": "2.2.5",
  "repository": "Theo-Messi/tmfe",
  "homepage": "https://tmfe.theojs.cn/",
  "funding": "https://github.com/Theo-Messi/tmfe?sponsor=1",
  "author": "Theo-Messi",
  "license": "MIT",
  "keywords": [
    "scss",
    "sass",
    "styles",
    "vue",
    "vite",
    "typescript",
    "vitepress"
  ],
  "main": "vue/index.ts",
  "style": "scss/all.scss",
  "exports": {
    ".": {
      "import": "./vue/index.ts"
    },
    "./theme": "./theme/all.scss",
    "./vue": "./vue/index.ts",
    "./Footer": "./vue/HomeFooter.vue"
  },
  "scripts": {
    "all": "vite build",
    "release": "bumpp && pnpm -r publish",
    "dev": "vitepress dev docs --host",
    "build": "vitepress build docs",
    "preview": "vitepress preview docs",
    "prettier": "prettier . --write",
    "check": "prettier . --check"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "@vitejs/plugin-vue": "^5.1.2",
    "lint-staged": "^15.2.8",
    "prettier": "3.3.3",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^2.0.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitepress": "^1.3.2"
  },
  "files": [
    "theme/",
    "vue/"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "bumpp": "^9.4.2",
    "sass": "^1.77.8",
    "vue": "^3.4.35"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "none"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "packageManager": "pnpm@9.7.0"
}
